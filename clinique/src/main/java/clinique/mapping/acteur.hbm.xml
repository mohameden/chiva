<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="clinique.mapping">
 <class name="Acteur" table="tbl_acteur">
 <id name="acteurId" column="acteur_id">
 <generator class="assigned"/>
 </id>

<property name="nom" column="nom" />
<property name="designation" column="designation" />
<property name="assistant" column="assistant" />
<property name="telephone" column="telephone" />
<property name="email" column="email" />
<property name="statut" column="statut" />
<property name="operateur" column="operateur" />

</class>
</hibernate-mapping>
